{
  "name": "@musicapi/sunoapi",
  "version": "1.0.2",
  "description": "A Node.js SDK for interacting with the SunoAPI to create and manage music using the Sonic model.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch", 
    "test:coverage": "jest --coverage",
    "prepare": "npm run build",
    "start": "node dist/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "homepage": "https://github.com/musicapi-ai/sunoapi-js-sdk",
  "repository": {
    "type": "git", 
    "url": "https://github.com/musicapi-ai/sunoapi-js-sdk.git"
  },
  "keywords": [
    "sunoapi",
    "sdk",
    "music",
    "nodejs"
  ],
  "author": "musicapi",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^18.0.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5.0.0"
  }
}
